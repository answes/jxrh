<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>

<AnchorPane fx:id="root" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1">
    <HBox prefWidth="200" AnchorPane.bottomAnchor="0" AnchorPane.leftAnchor="0" AnchorPane.rightAnchor="0" AnchorPane.topAnchor="0">
    <Label fx:id="time" alignment="CENTER_LEFT" prefHeight="30" style="-fx-font-size: 12px" text="11:00:00" textFill="#C9992D">
        <HBox.margin>
            <Insets left="4.0" right="12.0" />
        </HBox.margin></Label>
    <Label fx:id="title" alignment="CENTER_LEFT" prefHeight="30" style="-fx-font-size: 12px" text="新闻标题:门中惊醒梦中人" textFill="#C9992D" />
    </HBox>
</AnchorPane>
